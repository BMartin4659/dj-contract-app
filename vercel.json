{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "cleanUrls": true,
  "framework": "nextjs",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_ENV": "production"
    }
  },
  "env": {
    "NEXT_PUBLIC_VERCEL_URL": "https://$VERCEL_URL"
  }
} 